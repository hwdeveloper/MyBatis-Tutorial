    <!-- 保存用户 -->
    <insert id="saveUser" parameterType="com.ff.bean.User">
        insert into user(username,address,sex,birthday)values(#{userName},#{userAddress},#{userSex},#{userBirthday});
    </insert>

  
</mapper>