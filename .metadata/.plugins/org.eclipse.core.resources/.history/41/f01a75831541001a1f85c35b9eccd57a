package com.ff.po;

import java.util.List;

public class ClassRoom {
	/**
	 * 教室id
	 */
	private int id;

	/**
	 * 教室的姓名
	 */
	private String name;

	/**
	 * 老师的id
	 */
	private int teacherId;
	/**
	 * 学生列表
	 */
	private List<Student> students;
	
	/**
	 * 老师
	 */
	private Teacher teacher;

	public ClassRoom() {
		super();
	}

	
}
