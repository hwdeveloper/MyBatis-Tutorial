package com.ff.po;
public class ClassRoom {
	private int id;
	private String name;
	private String sex;
	private int age;
	private String pwd;
	@Override
	public String toString() {
		return "Teacher [id=" + id + ", name=" + name + ", sex=" + sex + ", age=" + age + ", pwd=" + pwd + "]";
	}
	public ClassRoom() {
		super();
	}
	public ClassRoom(String name, String sex, int age, String pwd) {
		super();
		this.name = name;
		this.sex = sex;
		this.age = age;
		this.pwd = pwd;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSex() {
		return sex;
	}
	public void setSex(String sex) {
		this.sex = sex;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
}
