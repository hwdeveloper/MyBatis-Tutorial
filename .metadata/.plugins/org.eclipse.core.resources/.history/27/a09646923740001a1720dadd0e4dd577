package com.ff.po;

public class ClassRoom {
	/**
	 * 教室id
	 */
	private int id;

	/**
	 * 教室的姓名
	 */
	private String name;

	/**
	 * 老师的id
	 */
	private int teacherId;
	
	
	public ClassRoom() {
		super();
	}

	public ClassRoom(int id, String name, int teacherId) {
		super();
		this.id = id;
		this.name = name;
		this.teacherId = teacherId;
	}

}
